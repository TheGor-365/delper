    <section class="command">
      <div class="git_command_list">
        <h1 class="display-3">Git commands list</h1>
      </div>
      <section class="git_init">
          <div class="jumbotron jumbotron-fluid">
            <button id="little_button" class="btn btn-primary" onclick="gitInitCopy()">copy</button>
            <div class="container">
              <h1 class="display-4">Git initialization</h1>
              <figure onclick="gitInitCopy()" class="form-control" id="git_init" value="git init">
                <pre>git init</pre>
              </figure>
            </div>

            <section class="details">
              <div class="accordion" id="accordionExample">
                <div class="card">
                  <div class="card-header" id="headingOne">
                    <h2 class="mb-0">
                      <button class="text-decoration-none btn btn-link" type="button" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                        Details
                      </button>
                    </h2>
                  </div>

                  <div id="collapseOne" class="collapse show" aria-labelledby="headingOne" data-parent="#accordionExample">
                    <div class="card-body">
                      git init - создает новый репозиторий Git
                    </div>
                  </div>
                </div>
              </div>
            </section>

          </div>
